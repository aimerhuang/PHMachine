                                                    </StackPanel>
                                                </Button>
                                                <Button Name="CmdH" Style="{DynamicResource InformButton}" VerticalContentAlignment="Top" Focusable="False" Click="Button_Click"  HorizontalContentAlignment="Left"  Width="60" Height="60">
                                                    <StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" >
                                                        <TextBlock    HorizontalAlignment="Left" VerticalAlignment="Top"   ><Run Text="H"/></TextBlock>
                                                        <TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom"   />
                                                    </StackPanel>
                                                </Button>

                                                <Button Name="CmdJ" Style="{DynamicResource InformButton}" VerticalContentAlignment="Top" Focusable="False" Click="Button_Click"  HorizontalContentAlignment="Left"  Width="60" Height="60">
                                                    <StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" >
                                                        <TextBlock    HorizontalAlignment="Left" VerticalAlignment="Top"   ><Run Text="J"/></TextBlock>
                                                        <TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom"   />
                                                    </StackPanel>
                                                </Button>

